#include<stdio.h>
int main()
{
  int c;
  for(int i=1; i<1000; i++)
    {
      for(int j=1; j<1000; j++)
	{
	  c=(i*i)+(j*j);
	  for(int k=1;k<1000; k++)
	    {
	      if(c==k*k)
		{
		  if(i+j+k==1000)
		    {
		      printf("%d*%d*%d-> %d\n",i,j,k,i*j*k);
		      
		    }
		}
	    }
	}
    }
  return 0;
}
